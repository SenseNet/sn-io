{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File", "Serilog.Sinks.Graylog" ],
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      //{ "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "logs/SenseNet.IO-.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  }
}